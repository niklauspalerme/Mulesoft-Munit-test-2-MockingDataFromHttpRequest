#%RAML 1.0
title: kcc-mkt-stratifyd-exp-api
baseUri: https://anypoint.mulesoft.com/mocking/api/v1/links/5a0d305b-9781-4218-afeb-60ca72233a10/ # baseUri:
version:  v1
protocols:
  - HTTPS
mediaType: 
  - application/json
traits:
  client-id-required:
    headers:
      client_id:
        type: string
      client_secret:
